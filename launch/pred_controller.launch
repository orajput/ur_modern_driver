<launch>
  <arg name="command_args" default="" />
  <arg name="bag_record" default="true" />
  
  <node pkg="rosbag" type="record" name="rosbag_record_cam"
       args="-o /data/ --lz4 /trk_rt_tf /ur_rt_tf"
       if="$(arg bag_record)" />

  <!-- Start the move_group action client -->
  <node name="test_move" pkg="ur_modern_driver" type="test_move.py" respawn="false" output="screen" args="$(arg command_args)" />
</launch>
